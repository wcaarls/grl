                            </axis>
                            <angle>0.5*_pi</angle>
                        </rotation>
                    </cylinder>
                </drawinfo> 
            </body>
            
            <!-- E: bindArm -->
            <body>
                <name>bindArm</name>
                <mass>mbindArm</mass>
                <IXX>irrelevantinertia</IXX>
                <IYY>irrelevantinertia</IYY>
                <IZZ>352081.22*scale3</IZZ>
                <anchor>
                    <name>three</name>
                    <x>0</x>
					<y>0</y>
					<z>lbindArm/2</z>
                </anchor>
                <anchor>
                    <name>four</name>
                    <x>0</x>
					<y>0</y>
					<z>-lbindArm/2</z>
                </anchor>
                <anchor>
                    <name>six</name>
                    <x>0</x>
					<y>0</y>
					<z>-lbindArm/2-lwaistdrop</z>
                </anchor>
                <drawinfo>
                    <cylinder>
                        <color>
							<r>0.3</r>
							<g>0.3</g>
							<b>0.3</b>
						</color>
                        <radius>radius</radius>
                        <length>lbindArm</length>
                    </cylinder>
                </drawinfo>  
            </body>
            
            <!-- F: waist -->
            <body>
                <name>waist</name>
                <mass>mwaist</mass>
                <IXX>irrelevantinertia</IXX>
                <IYY>235077.35*scale3</IYY>
                <IZZ>774701.67*scale3</IZZ>
                <anchor>
                    <name>six</name>
                    <x>0</x>
					<y>lwaist/2</y>
					<z>0</z>
                </anchor>
                <anchor>
                    <name>seven</name>
                    <x>0</x>
					<y>lwaist/4</y>
					<z>0</z>
                </anchor>
                <anchor>
                    <name>eight</name>
                    <x>0</x>
					<y>-lwaist/4</y>
					<z>0</z>
                </anchor>
                <drawinfo>
                    <cylinder>
                        <color>
							<r>0.3</r>
							<g>0.3</g>
							<b>0.3</b>
						</color>
                        <radius>radius</radius>
                        <length>lwaist</length>
                        <rotation>
                            <axis>
                                <x>1</x>
                                <y>0</y>
                                <z>0</z>
                            </axis>
                            <angle>0.5*_pi</angle>
                        </rotation>
                    </cylinder>
                </drawinfo>  
            </body>
           
            <!-- G: rightThigh -->
            <body>
                <name>rightThigh</name>    
                <mass>mthigh</mass>
                <IXX>214785.67*scale3</IXX>
                <IYY>128435.69*scale3</IYY>
                <IZZ>132908.93*scale3</IZZ>
                <anchor>
                    <name>seven</name>
                    <x>0</x>
					<y>0</y>
					<z>lthigh/2</z>
                </anchor>
                <anchor>
                    <name>nine</name>
                    <x>0</x>
					<y>0</y>
					<z>-lthigh/2</z>
                </anchor>
                <drawinfo>
                    <capsule>
                        <color>
							<r>169/256</r>
							<g>172/256</g>
							<b>182/256</b>
						</color>
                        <radius>radius</radius>
                        <length>lthigh - 2*radius</length>
                    </capsule>
                </drawinfo> 
            </body>
            
            <!-- H: leftThigh -->
            <body>
                <name>leftThigh</name>    
                <mass>mthigh</mass>
                <IXX>214785.67*scale3</IXX>
                <IYY>128435.69*scale3</IYY>
                <IZZ>132908.93*scale3</IZZ>
                <anchor>
                    <name>eight</name>
