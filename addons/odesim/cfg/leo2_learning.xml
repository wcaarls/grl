                    <bodyname>mainArm</bodyname> 
                    <anchorname>four</anchorname>
                </anchor2>
                <axisX>1</axisX>
				<axisY>0</axisY>
				<axisZ>0</axisZ>
		    </joint>
            
            <!-- 6: bindArm_waist -->
            <joint>
				<name>bindArm_waist</name>
