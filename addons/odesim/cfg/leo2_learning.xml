                    <anchorname>three</anchorname>
                </anchor2>
                <axisX>1</axisX>
				<axisY>0</axisY>
				<axisZ>0</axisZ>
		    </joint>
            
            <!-- 4: bindArm-mainArm -->
            <joint>
				<name>bindArm-mainArm</name>
                <type>hinge</type>
                <anchor1> 
